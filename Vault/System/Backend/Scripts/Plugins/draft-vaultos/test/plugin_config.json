{
  "pluginName": "VaultOS",
  "version": "1.3.0",
  "description": "Modular second-brain system for Obsidian",
  "author": "Charlie + VaultOS Core",
  "enabled": true,
  "modules": {
    "core": {
      "folderScaffolding": true,
      "folderNoteDashboards": true,
      "backendRefractor": true,
      "animatedHomepage": true,
      "yamlInjector": true,
      "autoLogger": true
    },
    "filesAndMedia": {
      "mediaSorting": true,
      "mediaDashboard": true,
      "dataviewIntegration": true
    },
    "notes": {
      "blockEngine": true,
      "blocks": {
        "brainDump": true,
        "events": true,
        "goals": true,
        "habit": true,
        "reflection": true,
        "quote": true,
        "question": true,
        "task": true
      }
    },
    "learnings": {
      "progressTracker": true,
      "skillTree": true,
      "nuggetBank": true
    },
    "system": {
      "scaffoldGenerator": true,
      "configMigrator": true,
      "pluginManager": true,
      "animationBundler": true,
      "summarizerGo": true,
      "validatorElixir": true,
      "mapGenLua": true,
      "analyticsR": true
    },
    "gamifiedNarrative": {
      "museumOfSelf": true,
      "trophyEngine": true,
      "progressMirror": true,
      "unlockables": true
    },
    "whimsicalUtilities": {
      "diceRoller": true,
      "cardDraw": true,
      "nameGenerator": true
    },
    "uiHelpers": {
      "hud": true,
      "stylesCss": true,
      "variablesCss": true,
      "snippetsCss": true,
      "styleGuideMd": true
    }
  },
  "defaultSettings": {
    "vaultRoot": "VAULT",
    "useDataview": true,
    "logFile": "System/Backend/Logs/vaultos-log.md",
    "defaultHomepage": "VAULT/System/Frontend/Dashboard/Homepage.md",
    "defaultYAMLVersion": "1.3"
  }
}
